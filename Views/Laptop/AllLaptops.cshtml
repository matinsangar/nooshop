@model IEnumerable<nooshop.Models.Laptop>

@{
    ViewBag.Title = "title";
    Layout = "_Layout";
}

<h2>ALL laptops will be here</h2>

<table border="1">
    <tr>
        <th>ID</th>
        <th>Brand</th>
        <th>Model</th>
    </tr>

    @foreach (var laptop in Model)
    {
        <tr>
            <td>@laptop.ProductID</td>
            <td>@laptop.Model</td>
            <td>@laptop.Name</td>
        </tr>
    }
</table>